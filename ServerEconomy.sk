command /bal [<text>]:
	trigger:
		if arg-1 is set:
			send "&cThis feature is disabled."
		else:
			set {_profile} to metadata tag "profile" of player
			set {_uuid} to uuid of player
			send "%nl%%nl%"
			send "&6Current Accounts"
			send "&e&lBank &eYou have %moneyComma({VaultBal::%{_uuid}%::%{_profile}%})%&e coins in your bank account."
			send "&e&lBalance &eYou have %moneyComma({Balance::%{_uuid}%::%{_profile}%})%&e coins in your balance."
			send "%nl%&aDeposit your balance into your bank account so you don't lose 25%% of it if you die!"
			send "%nl%%nl%"

command /pay <player> <integer>:
	trigger:
		set {_profileP} to metadata tag "profile" of player
		set {_profileA} to metadata tag "profile" of arg-1
		if arg-2 <= {Balance::%player's uuid%::%{_profileP}%}:
			add arg-2 to {Balance::%arg-1's uuid%::%{_profileA}%}
			remove arg-2 from {Balance::%player's uuid%::%{_profileP}%}
			send "&aYou have sent $%arg-2%&a to %arg-1%" to player
			send "&aYou have received $%arg-2%&a from %player%" to player
		else:
			send "&cYou don't have enough money to pay this amount!"

